#VRML V2.0 utf8

PROTO ImageViewer[exposedField SFInt32 id -1
	exposedField SFInt32 whichChoice 0
	exposedField MFString url []
	exposedField SFVec2f size 1 1
	exposedField SFNode thumbTexture NULL 
	exposedField SFBool showThumb TRUE 
	exposedField SFNode metadata NULL 
	exposedField SFString description ""
	eventOut SFTime on_click
	eventOut SFInt32 on_click_id
]
{Switch {whichChoice IS whichChoice
		choice[Group {
				children [
					DEF TS_1 TouchSensor {description IS description
						touchTime IS on_click
					}
				
					Shape {
						appearance LOD {
							range [10,50]
							level [
								Appearance {
									texture ImageTextureLOD {thumbTexture IS thumbTexture
										showThumb IS showThumb
										url IS url
										repeatS FALSE 
										repeatT FALSE 
										url IS url
										parameter "subtexture"
										thumbTexture IS thumbTexture
										showThumb IS showThumb
									}}
							
								Appearance {texture IS thumbTexture
									###material DEF _Material Material { transparency 0.5}
									texture IS thumbTexture
								}
							
								DEF _Appearance Appearance {material Material {transparency 0.7}}
							]
						}
						geometry Rectangle {
							size IS size}
					}
				]
			}]
		whichChoice IS whichChoice
	}
	DEF S_1 Script {
		eventIn SFTime touchTime
		eventOut SFInt32 on_click_id IS on_click_id
		exposedField SFInt32 id IS id
		url "javascript:
		function touchTime(t) {
		    on_click_id = id; 
		}
		"
	}
	ROUTE TS_1.touchTime_changed TO S_1.touchTime
}


